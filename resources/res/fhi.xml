<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.whatsapp.wds.components.topbar.WDSToolbar android:id="@+id/toolbar" android:background="?attr/(name removed)" android:layout_width="match_parent" android:layout_height="?attr/(name removed)" android:minHeight="?attr/(name removed)" style="@style/(name removed)"/>
    <com.whatsapp.KeyboardPopupLayout android:gravity="bottom" android:id="@+id/conversation_layout" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:id="@+id/footer_container" android:layout_width="match_parent" android:layout_height="match_parent">
            <ScrollView android:id="@+id/message_bubble_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/(name removed)" android:layout_above="@+id/edit_composer"/>
            <LinearLayout android:orientation="vertical" android:id="@+id/edit_composer" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true">
                <FrameLayout android:id="@+id/mention_attach" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/(name removed)" android:layout_marginBottom="-3.5dp"/>
                <include layout="@layout/(name removed)"/>
                <com.whatsapp.emoji.search.EmojiSearchContainer android:id="@+id/emoji_search_container" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            </LinearLayout>
        </RelativeLayout>
    </com.whatsapp.KeyboardPopupLayout>
</LinearLayout>
